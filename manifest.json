{
  "manifest_version": 3,
  "name": "Vocab Master",
  "description": "Reminds you vocabulary while you are surfing.",
  "version": "1.3.0",
  "author": "Ufuk Uzun",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "vocab-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "alarms",
    "storage"
  ],
  "icons": {
    "256": "vocab-icon.png"
  },
  "options_ui": {
    "page": "options.html"
  }
}